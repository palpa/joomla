<?xml version="1.0" encoding="utf-8"?>
<install version="1.7" type="plugin" group="search" method="upgrade" >
    <name>Vehicle Manager - Search</name>
    <creationDate>2011</creationDate>
    <author>OrdaSoft - Andrey Kvasnevsky</author>
    <authorEmail>akbet@ordasoft.com</authorEmail>
    <authorUrl>www.ordasoft.com</authorUrl>
    <copyright>(C) 2011 OrdaSoft - Andrey Kvasnevsky</copyright>
    <license>http://www.gnu.org/copyleft/gpl.html GNU/GPL</license>  
    <version>2.2</version>
    
    <description><![CDATA[ <b>VehicleManager Search</b> allows search vehicles from the site search.]]></description>
    
    <files>
      
        <filename plugin="VehiclemanagerSearch">VehiclemanagerSearch.php</filename>
        <filename plugin="VehiclemanagerSearch">VehiclemanagerSearch.xml</filename>
        <filename>VehiclemanagerSearch_j3.php</filename>
        <filename>VehiclemanagerSearch_j3.xml</filename>
      
    </files>
    
    
    <params>
        <param  name="search_limit"
                type="text"
                size="5"
                default="50"
                label="Search Limit"
                description="Number of Search items to return">
        </param>
    </params>
        
    
   	<config>
      <fields name="params">
          <fieldset name="basic">
              <field  name="search_limit"
                      type="text"
                      size="5"
                      default="50"
                      label="Search Limit" 
                      description="Number of Search items to return"/>
          </fieldset>
      </fields>
    </config>    
</install>
